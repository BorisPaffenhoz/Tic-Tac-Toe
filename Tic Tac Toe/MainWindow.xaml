<Window x:Class="Tic_Tac_Toe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tic_Tac_Toe"
        mc:Ignorable="d"
        Title="Tic Tac Toe" Height="500" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="166*"/>
            <ColumnDefinition Width="166*"/>
            <ColumnDefinition Width="166*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        <Button Content="Button_1_1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157"/>
        <Button Content="Button_1_2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Grid.Column="1"/>
        <Button Content="Button_1_3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Grid.Column="2"/>
        <Button Content="Button_2_1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Margin="0,156,0,0" Grid.RowSpan="2"/>
        <Button Content="Button_2_2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Margin="0,156,0,0" Grid.RowSpan="2" Grid.Column="1"/>
        <Button Content="Button_2_3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Grid.RowSpan="2" Grid.Column="2" Grid.Row="1"/>
        <Button Content="Button_3_1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Margin="0,155,0,0" Grid.RowSpan="2" Grid.Row="1"/>
        <Button Content="Button_3_2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Margin="0,0,0,-1" Grid.Row="2" Grid.Column="1"/>
        <Button Content="Button_3_3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Height="157" Margin="0,155,0,0" Grid.Row="1" Grid.Column="2" Grid.RowSpan="2"/>
    </Grid>
</Window>
